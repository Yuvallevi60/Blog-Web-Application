<ul>
    <button>
        <a href="/">Home</a>
    </button>
    <% if (locals.posts) { %>
        <% for (let i=0; i<posts.length; i++) { %>
            <form action="/postpage" method="POST">
                <input type="hidden" name="pIndex" value="<%= i %>">
                <button type="submit"> <%= posts[i]["pHead"] %> </button>    
            </form>
        <% } %>
    <% } %>  
</ul>